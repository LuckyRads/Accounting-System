<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.accountingsystem.controller.UsersController">
   <children>
      <ButtonBar layoutX="30.0" layoutY="366.0" prefHeight="64.0" prefWidth="560.0">
         <buttons>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#openMenu" prefHeight="0.0" prefWidth="242.0" text="Back">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="systemBtn" mnemonicParsing="false" prefHeight="43.0" text="System">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="usersBtn" minWidth="57.0" mnemonicParsing="false" prefHeight="0.0" text="Users">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </buttons>
      </ButtonBar>
      <Button fx:id="peopleBtn" layoutX="77.0" layoutY="152.0" mnemonicParsing="false" onAction="#openPeople" prefHeight="0.0" prefWidth="242.0" text="People">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
