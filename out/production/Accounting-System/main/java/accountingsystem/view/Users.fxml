<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.accountingsystem.controller.UsersController">
   <children>
      <ButtonBar layoutX="30.0" layoutY="366.0" prefHeight="64.0" prefWidth="560.0">
         <buttons>
            <Button fx:id="menuBtn" mnemonicParsing="false" onAction="#openMenu" prefHeight="0.0" prefWidth="242.0" text="Menu">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="peopleBtn" mnemonicParsing="false" onAction="#openPeople" prefHeight="0.0" prefWidth="242.0" text="People">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="companiesBtn" mnemonicParsing="false" onAction="#openCompanies" prefHeight="0.0" prefWidth="242.0" text="Companies">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
